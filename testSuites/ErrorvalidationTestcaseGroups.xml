<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite   parallel="tests"     name="Suite">
	
	
	
	<groups>
		<run>
			<include name="ErrorHandling" />
		</run>
		
	</groups>
	
	
	
	
	
	
  <test thread-count="5" name="End To End Test">
    <classes>
      <class name="qa.testceses.StandAloneTestCases"/>
    </classes>
  </test> <!-- Test -->
  
  <test thread-count="5" name="Error Validation">
    <classes>
      <class name="qa.testceses.ErrorValidation"/>
    </classes>
  </test> <!-- Test -->
  
</suite> <!-- Suite -->
